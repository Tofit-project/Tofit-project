package com.tofit.mvc.model.service;

import java.util.List;

import org.springframework.web.bind.annotation.RestController;

import com.tofit.mvc.model.dao.FeedImageDao;
import com.tofit.mvc.model.dto.FeedImage;

@RestController
public class FeedImageServiceImpl implements FeedImageService{
	
	private final FeedImageDao feedImageDao;
	
	public FeedImageServiceImpl(FeedImageDao feedImageDao) {
		this.feedImageDao = feedImageDao;
	}

	@Override
	public boolean registerFeedImages(List<FeedImage> feedImages) {
		return feedImageDao.insertFeedImages(feedImages);
	}

	@Override
	public List<FeedImage> getFeedImages(int feedId) {
		return null;
	}

	@Override
	public boolean deleteFeedImage(int imageId) {
		return false;
	}

}
