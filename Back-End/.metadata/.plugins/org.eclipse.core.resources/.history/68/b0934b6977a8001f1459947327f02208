package com.tofit.mvc.model.service;

import java.util.List;

import com.tofit.mvc.model.dto.Feed;

public interface FeedService {
	
	// 피드 작성
	public int registerFeed(Feed feed);
	
	// 피드 전체 조회
	public List<Feed> getFeedList();
	
	// 피드 상세 조회
	public Feed getFeedInfo(int feedId);
	
	// 피드 수정
	public int updateFeedInfo(int feedId);
	
	// 피드 삭제
	public int deleteFeedInfo(int feedId);
}
